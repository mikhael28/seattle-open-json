{
    "name": "seattle-open-json",
    "version": "1.1.0",
    "description": "Scraper for Seattle youth organization data using Jina",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist/**/*",
        "src/**/*",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "prepublishOnly": "npm run build",
        "scrape": "node scrape-organizations.js",
        "convert": "node scripts/markdown-to-json.js",
        "convert:single": "node scripts/markdown-to-json.js --single",
        "consolidate": "node scripts/consolidate-programs.js",
        "consolidate:pretty": "node scripts/consolidate-programs.js --pretty",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "type": "module",
    "keywords": [
        "scraper",
        "seattle",
        "youth"
    ],
    "author": "Michael Nightingale",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "typescript": "^5.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mikhael28/seattle-open-json"
    }
}